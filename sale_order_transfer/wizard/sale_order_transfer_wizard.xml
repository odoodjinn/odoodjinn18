<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sale_order_transfer_wizard_view_form" model="ir.ui.view">
        <field name="name">sale.order.transfer.wizard.view.form</field>
        <field name="model">sale.order.transfer.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="warehouse_id"/>
                </group>
                <footer>
                    <button string="Transfer" class="btn-primary" type="object" name="action_confirm_transfer"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>