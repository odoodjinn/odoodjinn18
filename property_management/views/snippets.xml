<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="external_snippet" inherit_id="website.snippets" name="Best seller Snippet">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
               position="after">
            <t t-snippet="property_management.property_list"/>
        </xpath>
    </template>
</odoo>